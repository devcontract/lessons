{
  "name": "tester",
  "version": "2.0",
  "description": "test extension for educational purpose",
  "permissions": ["activeTab","declarativeContent","storage"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery341.js","js/content.js"]
    }
  ],
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}